<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="fr.inria.rocq.services"
    android:versionCode="21"
    android:versionName="1.21" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="19" />

    <uses-permission android:name="fr.inria.arles.yarta.ACCESS_DATA" />

    <application
        android:name=".CalloutsApp"
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".AllCalloutsActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalloutActivity"
            android:label="@string/app_callout" />
        <activity
            android:name=".CreateActivity"
            android:label="@string/app_createcallout" />
    </application>

</manifest>
